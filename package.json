    "gulp-file-include": "*",
    "gulp-htmlclean": "*",
    "gulp-htmlmin": "*",
    "gulp-replace": "*",
    "gulp-sass": "*",
    "gulp-terser": "*",
    "node-sass": "*",
    "terser-webpack-plugin": "*",
    "webpack": "*",
    "webpack-stream": "*",
    "basic-ftp": "*",
    "recursive-readdir": "*"
  }
}